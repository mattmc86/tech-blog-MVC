{{!-- This is to display all blogs by current user --}}

<div class="blogs">
  <ul>
    {{#each blogs}}
      <li>
        <div class="dashboard-blog">
          <div class="blog-title">{{this.blog_title}}</div>
          <div class="blog-date">date tbc helper</div>
        </div>
      </li>
    {{/each}}
  </ul>
  <button class="add-btn">Add new post</button>
</div>