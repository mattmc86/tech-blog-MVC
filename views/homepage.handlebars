
{{!-- show all blogs by all users --}}


<ul>
  {{#each blogs}}
  <li>
<div class="card">
  <div class="card-header">
   {{this.blog_title}}
  </div>
  <div class="card-body">
    <blockquote class="blockquote mb-0">
      <p>{{this.description}}</p>
      <footer class="blockquote-footer">{{this.user_name}}</footer>
    </blockquote>
  </div>
</div>
</li>
{{/each}}
</ul>

